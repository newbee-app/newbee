{
  "name": "newbee-authenticator-data-access",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/newbee/authenticator/data-access/src",
  "prefix": "newbee",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/newbee/authenticator/data-access/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/newbee/authenticator/data-access/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/newbee/authenticator/data-access/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/newbee/authenticator/data-access/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/newbee/authenticator/data-access/**/*.ts",
          "libs/newbee/authenticator/data-access/**/*.html"
        ]
      }
    },
    "compodoc": {
      "executor": "@twittwer/compodoc:compodoc",
      "options": {
        "tsConfig": "libs/newbee/authenticator/data-access/tsconfig.lib.json",
        "outputPath": "dist/compodoc/newbee-authenticator-data-access",
        "watch": true
      },
      "configurations": {
        "json": {
          "exportFormat": "json"
        }
      }
    }
  },
  "tags": []
}
