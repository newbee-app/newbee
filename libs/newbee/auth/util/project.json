{
  "name": "newbee-auth-util",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/newbee/auth/util/src",
  "prefix": "newbee",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/libs/newbee/auth/util"],
      "options": {
        "project": "libs/newbee/auth/util/ng-package.json",
        "updateBuildableProjectDepsInPackageJson": true
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/newbee/auth/util/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/newbee/auth/util/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/newbee/auth/util"],
      "options": {
        "jestConfig": "libs/newbee/auth/util/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/newbee/auth/util/**/*.ts",
          "libs/newbee/auth/util/**/*.html"
        ]
      }
    },
    "compodoc": {
      "executor": "@twittwer/compodoc:compodoc",
      "options": {
        "tsConfig": "libs/newbee/auth/util/tsconfig.lib.json",
        "outputPath": "dist/compodoc/newbee-auth-util"
      },
      "configurations": {
        "json": {
          "exportFormat": "json"
        }
      }
    }
  },
  "tags": []
}
