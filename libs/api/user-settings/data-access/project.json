{
  "name": "api-user-settings-data-access",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/api/user-settings/data-access/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/api/user-settings/data-access",
        "tsConfig": "libs/api/user-settings/data-access/tsconfig.lib.json",
        "packageJson": "libs/api/user-settings/data-access/package.json",
        "main": "libs/api/user-settings/data-access/src/index.ts",
        "assets": ["libs/api/user-settings/data-access/*.md"],
        "updateBuildableProjectDepsInPackageJson": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/api/user-settings/data-access/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/libs/api/user-settings/data-access"
      ],
      "options": {
        "jestConfig": "libs/api/user-settings/data-access/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "compodoc": {
      "executor": "@twittwer/compodoc:compodoc",
      "options": {
        "tsConfig": "libs/api/user-settings/data-access/tsconfig.lib.json",
        "outputPath": "dist/compodoc/api-user-settings-data-access",
        "watch": true
      },
      "configurations": {
        "json": {
          "exportFormat": "json"
        }
      }
    }
  },
  "tags": []
}
