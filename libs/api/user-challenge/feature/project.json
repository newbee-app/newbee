{
  "name": "api-user-challenge-feature",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/api/user-challenge/feature/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/api/user-challenge/feature",
        "tsConfig": "libs/api/user-challenge/feature/tsconfig.lib.json",
        "packageJson": "libs/api/user-challenge/feature/package.json",
        "main": "libs/api/user-challenge/feature/src/index.ts",
        "assets": ["libs/api/user-challenge/feature/*.md"],
        "updateBuildableProjectDepsInPackageJson": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/api/user-challenge/feature/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/api/user-challenge/feature/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
