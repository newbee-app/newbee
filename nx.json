{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "npmScope": "newbee",
  "affected": {
    "defaultBase": "main"
  },
  "cli": {
    "packageManager": "npm"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": "ZTAxYWVkZmEtODU0MC00YzMxLWI1NDYtZDJhZDA1MmQxNjg2fHJlYWQtd3JpdGU="
      }
    }
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest",
      "buildable": true
    },
    "@nrwl/angular:component": {
      "style": "css"
    },
    "@nrwl/nest:library": {
      "buildable": true
    }
  },
  "defaultProject": "newbee",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build", "test"]
    },
    "test": {
      "dependsOn": ["^test", "lint"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "serve": {
      "dependsOn": ["build"]
    },
    "storybook": {
      "dependsOn": ["build-storybook"]
    }
  }
}
